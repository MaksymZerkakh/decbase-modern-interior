{
  "name": "interior-design",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "watch:scss": "node-sass src/app/sass/ -o src/app --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "compile:css": "node-sass src/app/sass/ -o src/app && stylelint 'src/app/**/*.css' --fix || true && stylelint 'src/app/**/*.css' --fix",
    "minify:css": "css-minify --file src/app/style.css -o src/app/ && mv src/app/style.min.css src/app/style.css"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "10.4.15",
    "compress-images": "^2.0.5",
    "css-minify": "^2.0.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "fg-loadcss": "^3.1.0",
    "gifsicle": "^5.2.1",
    "gsap": "^3.12.2",
    "next": "13.4.19",
    "node-sass": "^9.0",
    "pngquant-bin": "^6.0.1",
    "postcss": "8.4.29",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lazy-load": "^4.0.1",
    "react-slick": "^0.29.0",
    "sharp": "^0.32.6",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "3.3.3",
    "typeface-oswald": "^1.1.13"
  }
}
